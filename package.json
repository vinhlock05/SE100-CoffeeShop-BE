{
  "name": "se100-coffeeshop-be",
  "version": "1.0.0",
  "description": "Backend Express + TypeScript + Prisma + PostgreSQL for Coffee Shop Management System",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./dist && tsc && cp -r src/views dist/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:seed": "npx prisma db seed"
  },
  "_moduleAliases": {
    "~": "dist"
  },
  "prisma": {
    "seed": "tsx src/seeds/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.2.2",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.9.0",
    "@types/streamifier": "^0.1.2",
    "chalk": "^5.3.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
